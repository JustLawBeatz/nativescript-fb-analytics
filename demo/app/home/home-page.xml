<Page
	xmlns="http://schemas.nativescript.org/tns.xsd"
	loaded="pageLoaded"
	class="page"
  xmlns:ui="nativescript-fb-analytics"
>
  <ActionBar class="action-bar">
    <Label
      class="action-bar-title"
      text="Facebook Analytics"
    />
  </ActionBar>

  <StackLayout class="p-20">
    <Button
      tap="{{sendTestEvent}}"
      text="fire test event"
      fontSize="30"
      height="100"
      width="200"
      backgroundColor="#cccccc"
    />
  </StackLayout>
</Page>